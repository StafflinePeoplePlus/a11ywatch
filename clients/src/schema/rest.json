{
  "version": "0.1.0",
  "endpoint": "https://api.a11ywatch.com/",
  "routes": [
    {
      "method": "POST",
      "path": "crawl-stream",
      "headers": {
        "Transfer-Encoding": "chunked"
      }
    },
    {
      "method": "POST",
      "path": "login",
      "params": [
        {
          "name": "email",
          "type": "string"
        },
        {
          "name": "password",
          "type": "string"
        }
      ]
    },
    {
      "method": "POST",
      "path": "register",
      "params": [
        {
          "name": "email",
          "type": "string"
        },
        {
          "name": "password",
          "type": "string"
        }
      ]
    }
  ]
}
